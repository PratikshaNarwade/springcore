<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean class="com.jspiders.springcore.bean.CompanyBean" name="company1">
<property name="id" value="1"/>
<property name="name" value="Infosys"/>
<property name="address" value="Hinjewadi"/>
<property name="employees">
<list>
<bean class="com.jspiders.springcore.bean.EmployeeBean" name="employee1">
<property name="id" value="1"/>
<property name="name" value="Pratiksha"/>
<property name="email" value="pratiksha@gmail.com"/>
</bean>
<bean class="com.jspiders.springcore.bean.EmployeeBean" name="employee2">
<property name="id" value="2"/>
<property name="name" value="Abhi"/>
<property name="email" value="abhi@gmail.com"/>
</bean>
<bean class="com.jspiders.springcore.bean.EmployeeBean" name="employee3">
<property name="id" value="3"/>
<property name="name" value="Rohan"/>
<property name="email" value="rohan@gmail.com"/>
</bean>
</list>
</property>
</bean>
</beans>