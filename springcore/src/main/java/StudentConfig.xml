<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

 <bean class="com.jspiders.springcore.bean.StudentBean1" name="student1">
<property name="id">
 <value>1</value>
 </property>
 <property name="name" value="Pratiksha"/>
 <property name="email" value="pratiksha21@gmail.com"/>
 </bean>
 
 <bean class="com.jspiders.springcore.bean.StudentBean2" name="student2">
 <constructor-arg name="id" value="2"/>
 <constructor-arg name="name" value="Abhi"/>
 <constructor-arg name="email" value="abhi@gmail.com"/>
 </bean>
</beans>